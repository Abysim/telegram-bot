{
    "name": "abysim/telegram-bot",
    "type": "project",
    "description": "Personal bot with fishing game, MagaHAL-like AI, some admin functionality.",
    "keywords": ["telegram", "bot", "megahal", "fishing", "game", "admin"],
    "license": "MIT",
    "homepage": "https://github.com/abysim/telegram-bot",
    "support": {
        "issues": "https://github.com/abysim/telegram-bot/issues",
        "source": "https://github.com/abysim/telegram-bot"
    },
    "authors": [
        {
            "name": "Abysim",
            "homepage": "https://abysim.com/",
            "role": "Developer"
        },
        {
            "name": "PHP Telegram Bot Team",
            "homepage": "https://github.com/php-telegram-bot/example-bot/graphs/contributors",
            "role": "Developer"
        }
    ],
    "require": {
        "longman/telegram-bot": "0.76.1",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "monolog/monolog": "^2.2",
        "cijic/phpmorphy": "^0.3.1",
        "landrok/language-detector": "^1.3",
        "deeplcom/deepl-php": "^1.5",
        "openai-php/client": "^0.10.1",
        "aws/aws-sdk-php": "^3.312"
    },
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "scripts": {
        "check-code": [
            "\"vendor/bin/phpcs\""
        ]
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
